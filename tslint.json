{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "max-line-length": {
      "options": [90]
    }
  },
  "rules": {
    "max-line-length": [
      true,
      {
        "limit": 90,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ]
  },
  "rulesDirectory": []
}
